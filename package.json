{
  "name": "--cypress-udemy-",
  "version": "1.0.0",
  "description": "Automation of Test in Cypress",
  "main": "index.js",
  "scripts": {
    "test": "cypress open",
    "cy:run:edge": "cypress run   -b edge",
    "cy:run:chrome": "cypress run  -b chrome",
    "ui-allure": "cypress run --browser chrome --spec cypress/e2e/Test-Herokuapp/Test-Herokuapp.cy.js --env allure=true",
    "allure:report1": "allure generate allure-results --clean -o allure-report",
    "allure-report": "allure generate ./allure-results --clean",
    "test:allure": "npm run ui-allure && npm run report:allure",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",

    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
  },
  "author": "Elias Geronimo",
  "license": "ISC",
  "devDependencies": {
    "@cypress/xpath": "^2.0.2",
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "allure-commandline": "^2.20.1",
    "cypress": "^10.10.0",
    "cypress-xpath": "^2.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "xpath": "^0.0.32"
  }
}
